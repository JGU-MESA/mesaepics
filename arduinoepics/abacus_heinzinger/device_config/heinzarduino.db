# AOFFs f√ºr i_get, i_max_get und i_min_get gemessen, 
# am 20.07.2018 bei 100 kV, 1000 Samples, 0.2s delayq

record(stringout, "$(P)$(R)debug")
{
    field(DTYP, "stream")
    field(OUT, "@heinzarduino.proto debug $(PORT)")
}

#===== Samples =====
record(ai, "$(P)$(R)nos_get")
{
    field(DESC, "Get number of samples")
    field(DTYP, "stream")
    field(INP, "@heinzarduino.proto GetSamples $(PORT)")
    field(EGU,  "bits")
    field(PREC, "0")
    field(HOPR, "128")
    field(LOPR, "2")
}

record(ao, "$(P)$(R)nos_set")
{
    field(DESC, "Set number of samples")
    field(DTYP, "stream")
    field(OUT, "@heinzarduino.proto SetSamples $(PORT)")
    field(EGU,  "bits")
    field(PREC, "0")
    field(HOPR, "128")
    field(LOPR, "2")
    field(DRVH, "128")
    field(DRVL, "2")
}

#===== Current =====
record(ai, "$(P)$(R)i_get")
{
    field(DESC, "Get Current")
    field(DTYP, "stream")
    field(INP, "@heinzarduino.proto GetCurrAll $(PORT)")
    field(SCAN, ".1 second")
    field(EGU,  "A")
    field(ASLO, "1e-6") ## Device sends uA
    field(AOFF, "1.12e-05")
    field(PREC, "3")
    field(HOPR, "0.01")
    field(LOPR, "3e-7")
}

record(ai, "$(P)$(R)i_std_get")
{
    field(DESC, "Get std current")
    field(DTYP, "stream")
    field(INP, "@heinzarduino.proto GetCurrStd $(PORT)")
    field(SCAN, "I/O Intr")
    field(EGU,  "A")
    field(ASLO, "1e-6") ## Device sends uA
    field(PREC, "3")
    field(HOPR, "0.01")
    field(LOPR, "3e-7")
}

