record(stringout, "$(P)$(R)debug")
{
    field(DTYP, "stream")
    field(OUT, "@heinzarduino.proto debug $(PORT)")
}

#===== Current =====
record(ai, "$(P)$(R)i_get")
{
    field(DESC, "Get Current")
    field(DTYP, "stream")
    field(INP, "@heinzarduino.proto GetCurr $(PORT)")
    field(SCAN, ".2 second")
    field(EGU,  "A")
    field(ASLO, "1e-6") ## Device sends uA
    field(PREC, "3")
    field(HOPR, "0.01")
    field(LOPR, "3e-7")
}

