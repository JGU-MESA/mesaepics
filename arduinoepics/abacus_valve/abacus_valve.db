record(stringout, "$(P)$(R)debug")
{
    field (DTYP, "stream")
    field (OUT, "@abacus_valve.proto debug $(PORT)")
}

record(bo, "$(P)$(R)testled_set")
{
    field (DTYP, "stream")
    field (OUT, "@abacus_valve.proto testled_set $(PORT)")
    field (ZNAM, "Off")
    field (ONAM, "On")
    field (FLNK, "$(P)$(R)testled_get")
}

record(bi, "$(P)$(R)testled_get")
{
    field (DTYP, "stream")
    field (INP, "@abacus_valve.proto testled_get $(PORT)")
    field (ZNAM, "Off")
    field (ONAM, "On")
    field (PINI, "YES")
}

record (bi, "$(P)$(R)ls_get")
{
    field (DTYP, "stream")
    field (INP, "@abacus_valve.proto ls_on_get $(PORT)")
    field (SCAN, ".5 second")
    field (ZNAM, "Off")
    field (ONAM, "On")
}

record (bo, "$(P)$(R)ls_set")
{
    field (DTYP, "stream")
    field (OUT, "@abacus_valve.proto ls_on_set $(PORT)")
    field (ZNAM, "Off")
    field (ONAM, "On")
}


