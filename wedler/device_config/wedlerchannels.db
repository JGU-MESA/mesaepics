record(calc, "$(ELMNT)devi")
{
    field(DESC, "Diff nominal-actual")
    #field(INPA, "$(ELMNT)inom_get")    # Hajo 29.06.17
    field(INPA, "$(ELMNT)i_set")
    field(INPB, "$(ELMNT)i_get")
    field(INPC, "$(ELMNT)vrdy")
    field(INPD, "1000")
    field(CALC, "(A - B) * C * D")
    field(EGU,  "mA")
    #field(PREC, "6")
    field(HOPR, "1000")      #all settings from Hajo
    field(LOPR, "-1000")
    field(HIHI, "500")   
    field(LOLO, "-500")
    field(HIGH, "100")
    field(LOW,  "-100")
    #field(HOPR, "1100")     
    #field(LOPR, "-1100")
    #field(HIHI, "1000")
    #field(LOLO, "-1000")
    #field(HIGH, "900")
    #field(LOW,  "-900")
    field(LLSV, "MAJOR")
    field(LSV,  "MINOR")
    field(HSV,  "MINOR")
    field(HHSV, "MAJOR")
}

