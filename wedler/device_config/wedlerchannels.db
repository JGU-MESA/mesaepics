record(ao, "$(ELMNT)iwobble_set") 
{
    field(DESC, "Set DAC wobble value of a channel")
    field(DTYP, "Soft Channel")
    field(SCAN, ".1 second")
    field(EGU,  "A")
    field(PREC, "6")
}

record(calcout, "calc_wobble") {
  field(INPA, "$(ELMNT)iwobble_set CP")
  field(INPB, "$(ELMNT)iwobble_set.OVAL CP")    
  field(CALC,  "A<B?6:5")      #6=1sec, 5=2sec, 4=5sec, siehe Mamiwiki Doku ramp, soll nie aufhören
  field(OOPT, "On Change")
  field(OUT,  "$(ELMNT)iwobble_set.SCAN")
}

