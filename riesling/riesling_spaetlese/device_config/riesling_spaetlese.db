record(mbbo, "$(P)$(R)range_set")
{
    field (DTYP, "stream")
    field (OUT, "@riesling_spaetlese.proto set_range $(PORT)")
    field (ZRST, "Auto")
    field (ONST, "Fine")
    field (TWST, "Coarse")
}

record (ai, "$(P)$(R)i_get")
{
    field (DTYP, "stream")
    field (INP, "@riesling_spaetlese.proto read $(PORT)")
    field (SCAN, "I/O Intr")
    field (LINR, "LINEAR")
    field (ASLO, 1e-9)
    field (EGU, "mA")#raw input is pA
    field (PREC, 1)
    field (LOPR, "1e-9")
    field (HOPR, "2")
    
}
