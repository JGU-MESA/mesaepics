##==== Calc LaserPower ====
record(calc, "$(P)$(R)pow_get"){
    field(INPA, "$(POWERMETER)pow_get")
    field(INPB, "16.4140") 
    field(INPC, "18.878") 
    field(INPD, "$(P)$(R)on_get") 
    field(EGU, "W")
    field(CALC, "D = 1 ? (A < 75e-3 ? B  * A : C * A) : 0")
    field(SCAN, ".1 second")
    field(PREC, "2")
}

record(calc, "$(P)$(R)pow_att_get"){
    field(INPA, "$(POWERMETER)pow_get")
    field(INPB, "0.0309") 
    field(INPC, "0.0342") 
    field(INPD, "$(P)$(R)on_get") 
    field(EGU, "W")
    field(CALC, "D = 1 ? (A < 75e-3 ? B  * A : C * A) : 0")
    field(SCAN, ".1 second")
    field(PREC, "2")
}

