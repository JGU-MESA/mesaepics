#===== Amplitude =====
record(ai, "$(P)$(R)amp_get"){
    field(DESC, "Read amplitude")
    field(DTYP, "stream")
    field(INP, "@laser51.proto amp_get $(PORT)")
    field(HOPR, "65535")
    field(LOPR, "0")
    field(PREC, "0")
    field(EGU, "")
    field(HOPR, "65535")
    field(LOPR, "0")
    field(HIGH, "20000") #=1W laser power
    field(HIHI, "35000") #=3W laser power
    field(HHSV, "MAJOR") 
    field(HHSV, "MINOR")
    field(SCAN, "1 second")
}

record(calc, "$(P)$(R)setamp_get"){
    field(DESC, "Read the set amplitude at emission")
    field(INPA, "$(P)$(R)amp_get")
    field(SCAN, "Passive")
    field(HOPR, "65535")
    field(LOPR, "0")
    field(PREC, "0")
    field(EGU, "")
    field(HOPR, "65535")
    field(LOPR, "0")
    field(HIGH, "20000") #=1W laser power
    field(HIHI, "35000") #=3W laser power
    field(HHSV, "MAJOR") 
    field(HHSV, "MINOR")
    field(CALC, "A")
}

record(ao, "$(P)$(R)amp_set"){
    field(DESC, "Set amplitude")
    field(DTYP, "stream")
