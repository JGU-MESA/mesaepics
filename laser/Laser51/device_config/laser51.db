    field(DRVH, "40000")   #current Maximum set to 1mW
    field(DRVL, "1")
    field(HOPR, "65535")
    field(LOPR, "0")
    field(PREC, "0")
    field(EGU, "")    
#    field(FLNK, "$(P)$(R)amp_get.PROC")
}

