record (ai, "$(P)$(R)stat_get")
{
    field (DESC, "Get status of device")
    field (SCAN, "5 second")
    field (DTYP, "stream")
    field (INP, "@agilent.proto stat_get $(PORT)")
}

record (bi, "$(P)$(R)on_get")
{
    field (SCAN, "5 second")
    field (DTYP, "stream")
    field (INP, "@agilent.proto GetIonEnabled $(PORT)")
    field (ZNAM, "Off")
    field (ONAM, "On")
}

record (bo, "$(P)$(R)on_set")
{
    field (DTYP, "stream")
    field (OUT, "@agilent.proto SetIonEnabled $(PORT)")
    field (ZNAM, "Off")
    field (ONAM, "On")
    #field (FLNK, "$(P)$(R)SetIonEnabledMessage")
}

record (ai, "$(P)$(R)u_get")
{
    field (DESC, "Measure voltage on channel 1")
    field (SCAN, "1 second")
    field (DTYP, "stream")
    field (INP, "@agilent.proto GetVolt $(PORT)")
    field (EGU,  "V")
    field (LOPR, 0)
    field (HOPR, 7000)
}


record (ai, "$(P)$(R)i_get")
{
    field (DESC, "Measure current on channel 1")
    field (SCAN, "1 second")
    field (DTYP, "stream")
    field (INP, "@agilent.proto GetCurr $(PORT)")
    field (EGU,  "A")
    field (LOPR, 1e-10)
    field (HOPR, 9e-1)
}

record (ai, "$(P)$(R)p_get")
{
    field (DESC, "Measure current on channel 1")
    field (SCAN, "1 second")
    field (DTYP, "stream")
    field (INP, "@agilent.proto GetPress $(PORT)")
    field (EGU,  "MBAR")
    field (LOPR, 1e-12)
    field (HOPR, 1e-6)
}

