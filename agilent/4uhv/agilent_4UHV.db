record (waveform, "$(P)$(R)GetSerialNo")
{
    field (DESC, "Get serial number of device")
    field (DTYP, "stream")
    field (INP, "@agilent.proto GetSerialNo $(PORT)")
    field (SCAN, "Passive")
    field (PINI, "YES")
}

record (ai, "$(P)$(R)temp_get")
{
        field (DESC, "Get temperature of device in Degree Celsius")
        field (SCAN, "1 second")
        field (DTYP, "stream")
        field (INP, "@agilent.proto GetTemp $(PORT)")
    field (EGU, "DGC")
    field (LOPR, "0")
    field (HOPR, "200")
}

record (ai, "$(P)$(R)interlock_get")
{
        field (DESC, "Get status of the interlock")
#       field (SCAN, "1 second")
        field (DTYP, "stream")
        field (INP, "@agilent.proto GetInterlock $(PORT)")
}

record (ai, "$(P)$(R)GetErrorMessage")
{
        field (DESC, "Get error message from channel 1")
#       field (SCAN, "1 second")
        field (DTYP, "stream")
        field (INP, "@agilent.proto GetErrorMessage $(PORT)")
}

record (ao, "$(P)$(R)u_set")
{
    field (DESC, "Set voltage on channel 1 (def=7kV)")
#    field (SCAN, "1 second")
    field (DTYP, "stream")
    field (OUT, "@agilent.proto SetVolt $(PORT)")
    field (EGU,  "V")
    field (LOPR, 3000)
    field (HOPR, 7000)
}

