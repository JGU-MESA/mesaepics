record(bi, "$(P)$(R)ch$(Ch):stat_get") {
    #field(DESC, "Indicates if the high voltage for supply Ch$(Ch) is enabled")
    field(DTYP, "stream")
    field(INP, "@gv4qpc_channel.proto getHVStatus($(Ch)) $(PORT)")
    field(SCAN, "1 second") 
    #field(PINI, "YES")
    field(ZNAM, "NO")
    field(ONAM, "YES")
}

record(ai, "$(P)$(R)ch$(Ch):i_get") {
    field(DESC, "Reads pump current from supply Ch$(Ch)")
    field(DTYP, "stream")
    field(INP, "@gv4qpc_channel.proto rdCurrent($(Ch)) $(PORT)")
    field(SCAN, "1 second")
    #field(PINI, "YES")
    field(EGU, "A")
    field(HOPR, 1e-5)
    field(LOPR, 1e-10)
    field(PREC, 2)
}

record(ai, "$(P)$(R)ch$(Ch):p_get") {
    field(DTYP, "stream")
    field(INP, "@gv4qpc_channel.proto rdPressure($(Ch)) $(PORT)")
    field(SCAN, "1 second")
    #field(PINI, "YES")
    field(EGU, "mbar")
    field(HOPR, 1e-5)
    field(LOPR, 1e-10)
    field(PREC, 2)
}

record(ai, "$(P)$(R)ch$(Ch):u_get") {
    field(DTYP, "stream")
    field(INP, "@gv4qpc_channel.proto rdVoltage($(Ch)) $(PORT)")
    field(SCAN, "1 second")
    field(EGU, "V")
    field(PREC, 0)
}

record(stringin, "$(P)$(R)ch$(Ch):supplystatus_get") {
    field(DTYP, "stream")
    field(INP, "@gv4qpc_channel.proto rdSupplyStatus($(Ch)) $(PORT)")
    #field(SCAN, "5 second")
    #field(PINI, "YES")
}

record(bo, "$(P)$(R)ch$(Ch):on_set") {
    field(DTYP, "stream")
    field(OUT, "@gv4qpc_channel.proto startPump($(Ch)) $(PORT)") 
    field(ZNAM, "OFF")
    field(ONAM, "ON")
}
