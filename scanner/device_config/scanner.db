record(waveform,"$(P)$(R)DevIdent")
{
    field(DESC,"Get device idenfication of scanner")
    field(DTYP,"stream")
    field(INP,"@scanner.proto getDevIdent $(PORT)")
#    field(PINI,"YES")
    field(FTVL,"CHAR")
    field(NELM,"40")
}

