##===============================================
##=============== Enablegenerator ===============
## EG Input (Ch. 2.1.8.1)
record(mbbiDirect, "$(P)$(R)eg_input_$(FU)_get")
{
    field(DESC, "Get current active input")
    field(INP, "@scanner.proto getEnableGen($(FU),INPU) $(PORT)")
    field(DTYP, "stream")
    field(NOBT, "8")
    field(PINI, "YES")
}

record(mbboDirect, "$(P)$(R)eg_input_$(FU)_set")
{
    field(DESC, "Set current active input")
    field(DTYP, "stream")
    field(OUT, "@scanner.proto setEnableGen($(FU),INPU) $(PORT)")
    field(FLNK, "$(P)$(R)eg_input_$(FU)_get")
}


